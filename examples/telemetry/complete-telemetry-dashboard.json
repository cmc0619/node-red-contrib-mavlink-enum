        ]
    },
    {
        "id": "parse_esc_telemetry",
        "type": "function",
        "z": "mavlink_telemetry_dashboard",
        "name": "Parse ESC Telemetry",
        "func": "const esc = msg.payload || {};\nconst count = typeof esc.count === 'number' && esc.count > 0 ? esc.count : 4;\nconst rpm = Array.isArray(esc.rpm) ? esc.rpm : [];\nconst current = Array.isArray(esc.current) ? esc.current : [];\nconst temperature = Array.isArray(esc.temperature) ? esc.temperature : [];\nconst voltage = Array.isArray(esc.voltage) ? esc.voltage : [];\n\nconst table = [];\nconst rpmMsgs = [];\nconst currentMsgs = [];\nconst tempMsgs = [];\nconst voltMsgs = [];\n\nfor (let i = 0; i < count; i += 1) {\n    const escId = i + 1;\n    const rpmVal = typeof rpm[i] === 'number' ? rpm[i] : null;\n    const currentVal = typeof current[i] === 'number' ? current[i] / 100 : null; // cA \u2192 A\n    const tempVal = typeof temperature[i] === 'number' ? temperature[i] / 10 : null; // 0.1\u00b0C\n    const voltVal = typeof voltage[i] === 'number' ? voltage[i] / 100 : null; // 0.01V\n\n    table.push({\n        esc: escId,\n        rpm: rpmVal != null ? rpmVal : '\u2014',\n        current: currentVal != null ? currentVal.toFixed(2) : '\u2014',\n        temp: tempVal != null ? tempVal.toFixed(1) : '\u2014',\n        voltage: voltVal != null ? voltVal.toFixed(2) : '\u2014'\n    });\n\n    if (rpmVal != null) rpmMsgs.push({ payload: rpmVal, topic: `ESC ${escId}` });\n    if (currentVal != null) currentMsgs.push({ payload: currentVal, topic: `ESC ${escId}` });\n    if (tempVal != null) tempMsgs.push({ payload: tempVal, topic: `ESC ${escId}` });\n    if (voltVal != null) voltMsgs.push({ payload: voltVal, topic: `ESC ${escId}` });\n}\n\nreturn [\n    [{ payload: table }],\n    rpmMsgs,\n    currentMsgs,\n    tempMsgs,\n    voltMsgs\n];",
        "outputs": 5,
        "timeout": 0,
        "noerr": 0,
        "initialize": "",
        "finalize": "",
        "libs": [],
        "x": 430,
        "y": 620,
        "wires": [
            [
                "esc_summary"
            ],
            [
                "esc_rpm"
            ],
            [
                "esc_current"
            ],
